{% extends 'base.html.twig' %}

{% block title %}{{ parent() }} - Liste d'utilisateur{% endblock %}

{% block main %}

    <h2>Liste des utilisateurs</h2>

    {{ form_start(import_form) }}
        {{ form_errors(import_form) }}
        {{ form_widget(import_form) }}
    {{ form_end(import_form) }}

    <ul>
        {% for user in users %}

            <li><a href="{{ path('app_update', {id: user.id}) }}">{{ user.name | upper }} {{ user.firstName }} </a></li>

        {% endfor %}
    </ul>

{% endblock %}